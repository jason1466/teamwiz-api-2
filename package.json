{
  "name": "teamwiz-api",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/functions": "1.0.3",
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "6.3.1",
    "@nestjs/graphql": "6.2.4",
    "@nestjs/microservices": "6.3.1",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.3.1",
    "@types/passport-azure-ad": "4.0.2",
    "adal-node": "0.1.28",
    "adal-ts": "0.6.0",
    "apollo-server-express": "2.6.2",
    "azure-function-express": "2.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.9.1",
    "file-system": "^2.2.2",
    "graphql": "14.3.1",
    "graphql-subscriptions": "1.1.0",
    "mongoose": "5.6.3",
    "passport": "0.4.0",
    "passport-azure-ad": "4.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "type-graphql": "0.17.4"
  },
  "devDependencies": {
    "@types/node": "10.14.7",
    "@types/mongoose": "5.5.5",
    "nodemon": "1.18.11",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.5.1"
  },
  "keywords": []
}